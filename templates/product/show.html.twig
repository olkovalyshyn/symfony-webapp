{% extends "base.html.twig" %}

{% block body %}
	{% for item in product %}
		<h3>Product -
			{{ loop.index}}

		</h3>
		<ul>
			<li>Name -
				{{ item.name }}</li>

			<li>Price -
				{{ item.price }}</li>

			<li>Description -
				{{ item.description }}</li>

		</ul>
	{% endfor %}
{% endblock %}
